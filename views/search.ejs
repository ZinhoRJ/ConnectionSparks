<div class="author">
    <h1 class="author__heading">Resultados</h1>
</div>

<section class="articles">
    <ul class="article_ul">
        <% data.forEach(post => { %>
        <li>
            <a href="/post/<%= post._id %>">
                <span><%= post.title %></span>
                <span class="aticle-list_date"><%= post.createdAt.toDateString() %></span>
            </a>
        </li>
        <% }) %>
    </ul>
</section>